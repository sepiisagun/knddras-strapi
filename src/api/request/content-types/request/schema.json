{
  "kind": "collectionType",
  "collectionName": "requests",
  "info": {
    "singularName": "request",
    "pluralName": "requests",
    "displayName": "Request"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date",
      "required": true
    },
    "slot": {
      "type": "time",
      "required": true
    },
    "notes": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "ACCEPTED",
        "REJECTED"
      ],
      "default": "PENDING",
      "required": true
    },
    "purpose": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::procedure.procedure"
    },
    "patient": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
