      "required": true
    },
    "purpose": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::procedure.procedure"
    },
    "patient": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "request": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::request.request"
    }
  }
}
